@section scripts {
    <script src="~/js/create-1.js"></script> 
    <script type="">
        $(".DOI").daterangepicker({
            autoUpdateInput: false,
            singleDatePicker: true,
            showDropdowns: true,
            minYear: 1901,
            maxYear: parseInt(moment().format("YYYY"), 10)
        }
        );
        $(".DOI").on('apply.daterangepicker', function (ev, picker) {
            $(this).val(picker.startDate.format('MM/DD/YYYY'));
        });

        $(".DOE").daterangepicker({
            autoUpdateInput: false,
            singleDatePicker: true,
            showDropdowns: true,
            minYear: 1901,
            maxYear: parseInt(moment().format("YYYY"), 10)
        }
        );

        $(".DOE").on('apply.daterangepicker', function (ev, picker) {
            $(this).val(picker.startDate.format('MM/DD/YYYY'));
        });





        function ValidateVATNo() {
            if ($("#VATNo").val() != "") {
                $.ajax({
                    type: "POST",
                    async: true,
                    url: "/Home/VATValidate",
                    data: { VATNumber: $("#VATNo").val() },

                    error: function (x, e) {
                        var msg = "";
                        if (x.status == 0) {
                            msg = 'You are offline!!\n Please Check Your Network.';
                        } else if (x.status == 404) {
                            msg = 'Requested URL not found.';
                        } else if (x.status == 500) {
                            msg = 'Internel Server Error.';
                        } else if (e == 'parsererror') {
                            msg = 'Error.\nParsing JSON Request failed.';
                        } else if (e == 'timeout') {
                            msg = 'Request Time out.';
                        } else {
                            msg = 'Unknow Error.\n' + x.responseText;
                        }

                        Swal.fire({
                            text: msg,
                            icon: "error",
                            buttonsStyling: false,
                            confirmButtonText: "Ok, got it!",
                            customClass: {
                                confirmButton: "btn btn-primary"
                            }
                        });
                    },
                    success: function (d) {
                        console.log(d);
                        if (d == "Valid VAT Number") {
                            $("#VATNo").removeClass("is-invalid");
                            $("#opVATNo").text(d)
                            $("#opVATNo").addClass("text-success")
                            $("#opVATNo").removeClass("text-danger")
                        } else {
                            $("#VATNo").addClass("is-invalid");
                            $("#opVATNo").text(d)
                            $("#opVATNo").removeClass("text-success")
                            $("#opVATNo").addClass("text-danger")
                        }
                    }
                });
            }
        }

        function ValidateIBANNo(rowno) {
            if ($("#iban" + rowno).val() != "") {
                $.ajax({
                    type: "POST",
                    async: true,
                    url: "/Home/IBANValidate",
                    data: { IbanNumber: $("#iban" + rowno).val() },

                    error: function (x, e) {
                        var msg = "";
                        if (x.status == 0) {
                            msg = 'You are offline!!\n Please Check Your Network.';
                        } else if (x.status == 404) {
                            msg = 'Requested URL not found.';
                        } else if (x.status == 500) {
                            msg = 'Internel Server Error.';
                        } else if (e == 'parsererror') {
                            msg = 'Error.\nParsing JSON Request failed.';
                        } else if (e == 'timeout') {
                            msg = 'Request Time out.';
                        } else {
                            msg = 'Unknow Error.\n' + x.responseText;
                        }

                        Swal.fire({
                            text: msg,
                            icon: "error",
                            buttonsStyling: false,
                            confirmButtonText: "Ok, got it!",
                            customClass: {
                                confirmButton: "btn btn-primary"
                            }
                        });
                    },
                    success: function (d) {
                        console.log(d);
                        if (d == "Valid IBAN Number") {
                            $("#iban" + rowno).removeClass("is-invalid");
                            $("#opiban" + rowno).text(d)
                            $("#opiban" + rowno).addClass("text-success")
                            $("#opiban" + rowno).removeClass("text-danger")
                        } else {
                            $("#iban" + rowno).addClass("is-invalid");
                            $("#opiban" + rowno).removeClass("text-success")
                            $("#opiban" + rowno).addClass("text-danger")
                            $("#opiban" + rowno).text(d)
                        }
                    }
                });
            }
